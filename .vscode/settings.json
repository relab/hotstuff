{
	"protoc": {
		
        	"compile_on_save": false,
	        "options": [
				"--proto_path=${workspaceRoot}",
	            "--proto_path=${workspaceRoot}/internal/proto/hotstuffpb/",
        	    "--proto_path=`go list -m -f {{.Dir}} github.com/relab/gorums`",
        	]
    	},
	"go.testFlags": [
		"-count=1"
	],
	"cSpell.enabled": true,
	"cSpell.showStatus": true,
	"cSpell.customWorkspaceDictionaries": [
		{
			"name": "hotstuff",
			"addWords": true,
			"path": "${workspaceRoot}/.vscode/dict.txt",
		}
	]
}
